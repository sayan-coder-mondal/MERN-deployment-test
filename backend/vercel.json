{
    "instellCommand":"npm install --legacy-peer-deps",
    "version": 2,
    "name": "test-mern-deploy",
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}